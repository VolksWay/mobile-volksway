<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#12264B"
        android:padding="25dp"

>


    <TextView
            android:text="Checklist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_checklist"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="35sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="645dp"/>
    <TextView
            android:text="Ar Condicionado"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:id="@+id/text_ar"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_checklist"
            android:layout_marginTop="30dp"
            android:layout_marginStart="10dp"/>


    <TextView
            android:text="Freios"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_Freios"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintTop_toBottomOf="@+id/text_Documentos"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            android:layout_marginStart="10dp"/>

    <TextView
            android:text="Documentos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_Documentos"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_ar" android:layout_marginTop="30dp"
            android:layout_marginStart="10dp"/>
    <TextView
            android:text="Combustivel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_Combustivel"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_Oleo"
            android:layout_marginTop="30dp"
            android:layout_marginStart="10dp"/>
    <TextView
            android:text="Oleo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_Oleo"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_Freios"
            android:layout_marginStart="10dp" android:layout_marginTop="30dp"/>


    <TextView
            android:text="Pneu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_Peneu"
            android:textColor="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:textSize="25sp"
            app:layout_constraintTop_toBottomOf="@+id/text_Combustivel"
            android:layout_marginTop="52dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewResultado"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@+id/text_Peneu"
            android:layout_marginTop="52dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewChecklist"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@+id/text_Peneu"
            android:layout_marginTop="52dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"/>

    <ImageView
            android:layout_width="57dp"
            android:layout_height="58dp"
            app:srcCompat="@drawable/camera"
            android:id="@+id/camera"
            app:layout_constraintTop_toBottomOf="@+id/checkBox5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="208dp"
            app:layout_constraintVertical_bias="0.508" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.954"/>
    <CheckBox

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox3"
            app:layout_constraintStart_toEndOf="@+id/text_Freios"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938" app:layout_constraintTop_toBottomOf="@+id/checkBox10"
            android:layout_marginTop="32dp"/>
    <CheckBox

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox4"
            app:layout_constraintTop_toBottomOf="@+id/checkBox3"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="28dp"
            app:layout_constraintStart_toEndOf="@+id/text_Oleo" app:layout_constraintHorizontal_bias="0.942"/>
    <CheckBox

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox5"
            app:layout_constraintStart_toEndOf="@+id/text_Combustivel"
            app:layout_constraintTop_toBottomOf="@+id/checkBox4" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.904" android:layout_marginTop="15dp"/>
    <CheckBox

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox9"
            app:layout_constraintTop_toBottomOf="@+id/text_checklist"
            app:layout_constraintStart_toEndOf="@+id/text_ar" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="20dp" app:layout_constraintHorizontal_bias="0.859"/>
    <CheckBox

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox10"
            app:layout_constraintStart_toEndOf="@+id/text_Documentos" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.903"
            app:layout_constraintTop_toBottomOf="@+id/checkBox9" android:layout_marginTop="30dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>